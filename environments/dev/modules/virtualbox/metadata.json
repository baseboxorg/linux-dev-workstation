{
  "name": "puppet-virtualbox",
  "version": "1.8.0",
  "author": "Vox Pupuli",
  "summary": "Installs Virtualbox on RHEL and Debian-like systems",
  "license": "Apache-2.0",
  "source": "https://github.com/voxpupuli/puppet-virtualbox",
  "project_page": "https://github.com/voxpupuli/puppet-virtualbox",
  "issues_url": "https://github.com/voxpupuli/puppet-virtualbox/issues",
  "dependencies": [
    {"name":"puppetlabs/stdlib","version_requirement":">= 4.8.0 <5.0.0"},
    {"name":"puppetlabs/apt","version_requirement":">= 2.1.0 <3.0.0"},
    {"name":"stahnma/epel","version_requirement":">= 1.0.0 <2.0.0"}
